<h1>Recent</h1>

{{#each}}
  <div class="photo img-thumbnail">
    <div style="text-align: center;">
      {{#link-to 'recent.preview' id title=originalFilename}}
        <img class="photo-thumbnail" {{bind-attr src=small_photo_path}}>
      {{/link-to}}
    </div>
    {{partial 'thumb_details'}}
  </div>
{{/each}}
