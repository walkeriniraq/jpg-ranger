<div class="row">
  <div class="col-sm-9">
    {{#photo-grid photos=model selected_photos=selected_photos open_preview='open_preview' change_page='change_page'}}
      {{photo-upload upload_data=photo_upload_data start_upload='start_upload' end_upload='end_upload' file_uploaded='file_uploaded'}}
      <h1 style="text-align: center;">{{title}}</h1>
    {{/photo-grid}}
  </div>

  <div class="col-sm-3">
    {{partial 'photo/tagging'}}
  </div>
</div>
