<h1>{{controllers.tag.tag_name}}</h1>

{{#each}}
  <div class="photo img-thumbnail">
    <div style="text-align: center;">
      {{#link-to 'tag.preview' id title=originalFilename}}
        <img class="photo-thumbnail" {{bind-attr src=small_photo_path}}>
      {{/link-to}}
    </div>
    {{partial 'thumb_details'}}
  </div>
{{/each}}
