<h1>Recent</h1>

{{#each}}
  <div class="photo img-thumbnail" data-id="<%= photo.id %>">
    <div style="text-align: center;">
      <a {{bind-attr href=id title=original_filename}}>
        <img class="photo-thumbnail" {{bind-attr src=small_photo_path}}>
      </a>
    </div>
    <div {{bind-attr title=formattedPhotoTime}}>{{prettyPhotoTime}}</div>
    <div class="tag-holder" style="text-align: center">
      {{#each tags}}
        <a href="tag" class="tag label label-primary">
          {{this}}
        </a>
      {{/each}}
    </div>
  </div>
{{/each}}