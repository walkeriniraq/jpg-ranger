{{#each row in view.table}}
  <div class="row">
    {{#each photo in row}}
      <div class="col-lg-2 col-md-3 col-sm-4" style="text-align: center; padding-top: 1em;">
        <div class="photo img-thumbnail">
          <div style="text-align: center;">
            {{#link-to preview_route photo.id title=photo.original_filename}}
              <img class="photo-thumbnail" {{bind-attr src=photo.small_photo_path}}>
            {{/link-to}}
          </div>
          {{render 'tag_holder' photo}}
        </div>
      </div>
    {{/each}}
  </div>
{{/each}}
<div class="paging_bar">
  {{#if has_previous_page}}
    <span class="pull-left">Previous</span>
  {{/if}}
  Page {{current_page}} of {{total_pages}}
  {{#if has_next_page}}
    <span class="pull-right" {{action 'next_page'}}>Next</span>
  {{/if}}
</div>
